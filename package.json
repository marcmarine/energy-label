{
  "name": "energy-label",
  "version": "1.0.0-beta.8",
  "description": "Generate EU-compliant energy labels in SVG format.",
  "homepage": "https://docs.label.energy",
  "repository": {
    "type": "git",
    "url": "https://github.com/marcmarine/energy-label.git"
  },
  "main": "dist/index.js",
  "exports": {
    "import": "./dist/index.js"
  },
  "types": "dist/index.d.ts",
  "type": "module",
  "scripts": {
    "test": "vitest",
    "build:example": "bun build-example.ts",
    "build:docs": "bunx typedoc",
    "build:types": "dts-bundle-generator -o dist/index.d.ts src/index.ts",
    "build:lib": "rolldown --config rolldown.config.mjs",
    "build": "bun build:lib && bun build:types",
    "dev": "bun build:lib -- --watch"
  },
  "bugs": "https://github.com/marcmarine/energy-label/issues/new",
  "author": "Marc Marin√© <shenobi@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.3",
    "@semantic-release/git": "^10.0.1",
    "@types/qrcode": "^1.5.5",
    "dts-bundle-generator": "^9.5.1",
    "rolldown": "^1.0.0-beta.6",
    "semantic-release": "^24.2.3",
    "typedoc": "^0.28.5",
    "typescript": "^5.8.2",
    "upgradeps": "^2.0.7",
    "varvara-typedoc-theme": "^0.3.6",
    "vitest": "^3.0.9"
  },
  "dependencies": {
    "qrcode": "^1.5.4",
    "svgo": "^4.0.0-rc.1"
  },
  "files": [
    "dist"
  ],
  "keywords": [
    "energy",
    "labels",
    "generator",
    "smartphones",
    "european-union",
    "great-britain",
    "efficiency"
  ]
}
